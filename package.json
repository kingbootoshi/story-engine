{
  "name": "world-story-engine",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"npm run server\"",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "server": "tsx src/server.ts",
    "server:debug": "DEBUG=true tsx src/server.ts",
    "dev:all": "concurrently \"vite\" \"npm run server\"",
    "dev:debug": "concurrently \"vite\" \"npm run server:debug\"",
    "test": "vitest",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "tsc --noEmit",
    "validate-schema": "ts-node scripts/validate-schema.ts",
    "lint-logs": "ts-node scripts/lint-logs.ts"
  },
